{
  "expo": {
  	"name": "Always Sunny Soundboard",
    "sdkVersion": "27.0.0",
    "icon": "./assets/img/icon.png",
    "description": "A soundboard for It's Always Sunny in Philadelphia.",
    "version": "1.0.1",
    "orientation": "portrait",
    "ios": {
    	"bundleIdentifier": "com.jackpan.alwayssunnysoundboard"
